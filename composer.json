{
    "name": "fourmix/log_system",
    "description": "The Fourmix.Co.,Ltd's Log System.",
    "keywords": ["Log System", "laravel", "Fourmix"],
    "license": "MIT",
    "type": "project",
    "homepage": "http://www.fourmix.co.jp",
    "authors": [
        {
            "name": "Fourmix.Co.,Ltd",
            "email": "fourmix-system@fourmix.co.jp",
            "role": "Company"
        },
        {
            "name": "Wu Jie",
            "email": "kgo@fourmix.co.jp",
            "role": "Developer"
        },
        {
            "name": "Sakura Tanaka",
            "email": "stanaka@fourmix.co.jp ",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
